{
  "name": "playvideo",
  "version": "1.0.0",
  "description": "Official PlayVideo SDK for JavaScript/TypeScript - Video hosting for developers",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./webhooks": {
      "types": "./dist/webhooks.d.ts",
      "import": "./dist/webhooks.mjs",
      "require": "./dist/webhooks.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "playvideo",
    "video",
    "streaming",
    "hls",
    "transcoding",
    "video-hosting",
    "api",
    "sdk"
  ],
  "author": "PlayVideo <support@playvideo.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PlayVideo-dev/playvideo-js.git"
  },
  "bugs": {
    "url": "https://github.com/PlayVideo-dev/playvideo-js/issues"
  },
  "homepage": "https://playvideo.dev",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
